        <Canvas x:Name="CanvasBack" Width="640" Height="480" />
        <Viewport3D x:Name="View" Width="640" Height="480" />
        <Canvas x:Name="CanvasFront" Width="640" Height="480" />
    </Canvas>
</Window>
